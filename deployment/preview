#/bin/bash
# stop on error
set -e
export APP_NAME=preview-explorer
# in preview we deploy as if it was prod
export CONFIG_FILE_NAME="explorer.prod.yml"
export PREVIEW=1
export PLATFORM_URL=invariantlabs.ai
export DEV_MODE=false
export BASE_URL=invariantlabs.ai
docker compose --env-file .env.prod -f docker-compose.prod.yml $@