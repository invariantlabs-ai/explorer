#/bin/bash
# stop on error
set -e
export APP_NAME=preview-explorer
# in preview we deploy as if it was prod
export CONFIG_FILE_NAME="explorer.prod.yml"
PREVIEW=1 PLATFORM_URL=invariantlabs.ai APP_NAME=$APP_NAME DEV_MODE=false BASE_URL=invariantlabs.ai docker compose --env-file .env.prod -f docker-compose.prod.yml $@